<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0"
              xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
  
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>clientDialog</from-outcome>
            <to-view-id>/client/clientDialog.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>      
    
    <managed-bean>
        <managed-bean-name>labelManager</managed-bean-name>
        <managed-bean-class>jm.com.dpbennett.cm.manager.LabelManager</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    

</faces-config>
